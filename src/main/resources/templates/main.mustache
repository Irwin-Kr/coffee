{{>layout/header}}
		<ul class="nav nav-tabs" id="myTab" role="tablist">
		{{#brandList}}
		  <li class="nav-item" role="presentation">
		    <button class="nav-link" id="{{name}}-tab" data-bs-toggle="tab" data-bs-target="#{{name}}-tab-pane" type="button" role="tab" aria-controls="{{name}}-tab-pane" aria-selected="false">{{name}}</button>
		  </li>
	    {{/brandList}}
		</ul>
		<div class="tab-content" id="myTabContent">
		{{#brandList}}
		  <div class="tab-pane fade" id="{{name}}-tab-pane" role="tabpanel" aria-labelledby="{{name}}-tab" tabindex="0">
		  {{name}}
		  </div>
			<div class="card" style="width: 18rem;">
				<img src="..." class="card-img-top" alt="...">
				<div class="card-body">
					<h5 class="card-title">Card title</h5>
					<p class="card-text">Some quick example text to build on the card title and make up the bulk of the card's content.</p>
					<a href="#" class="btn btn-primary">상세 정보</a>
					<a href="#" class="btn btn-secondary">수정</a>
				</div>
			</div>
		{{/brandList}}
		</div>
{{>layout/footer}}	